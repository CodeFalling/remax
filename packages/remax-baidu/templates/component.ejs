<template name="REMAX_TPL_<%=templateId%>_<%=id%>">
  <%_ function propsKeyTpl(props, ignorekey) {
    // 互斥事件
    if (ignorekey) {
      props = props.filter(key => key !== ignorekey)
    }
    return props
      .map(key => {
        if (key === 'style') {
          return `${key}="{{_h.st(i.props['${key}'])}}"`
        } else {
          return `${key}="{{_h.v(i.props['${key}'])}}"`
        }
      })
      .join('\n')
  } _%>
  <%_ if (id === 'view') { _%>
  <%# view 标签需要区分是否含 catchtouchmove 事件，区分渲染模板 %>
  <block s-if="{{i.props['catchtouchmove']}}">
    <<%=id%> <%- propsKeyTpl(props, 'bindtouchmove') %>>
      <%- include('./children.ejs', { props: props, id: id, type: type, slotView: slotView }) %>
    </<%=id%>>
  </block>
  <block s-else>
    <<%=id%> <%- propsKeyTpl(props, 'catchtouchmove') %>>
      <%- include('./children.ejs', { props: props, id: id, type: type, slotView: slotView }) %>
    </<%=id%>>
  </block>
  <%_ } else if (id === 'label') { _%>
  <%# label 标签对 for 属性为空，导致嵌套使用无效 %>
  <block s-if="{{i.props['for']}}">
    <<%=id%> <%- propsKeyTpl(props) %>>
      <%- include('./children.ejs', { props: props, id: id, type: type, slotView: slotView }) %>
    </<%=id%>>
  </block>
  <block s-else>
    <<%=id%> <%- propsKeyTpl(props, 'for') %>>
      <%- include('./children.ejs', { props: props, id: id, type: type, slotView: slotView }) %>
    </<%=id%>>
  </block>
  <%_ } else {_%>
  <<%=id%> <%- propsKeyTpl(props) %>>
    <%- include('./children.ejs', { props: props, id: id, type: type, slotView: slotView }) %>
  </<%=id%>>
  <%_ } _%>
</template>
