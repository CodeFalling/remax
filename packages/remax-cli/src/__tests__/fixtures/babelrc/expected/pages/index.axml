    animation="{{item.props['animation']}}"
    class="{{item.props['class']}}"
    disable-scroll="{{item.props['disable-scroll']}}"
    hidden="{{item.props['hidden']}}"
    hover-class="{{item.props['hover-class']}}"
    hover-start-time="{{item.props['hover-start-time']}}"
