<% camelCased = function(str) { return str.replace(/-([a-z0-9])/g, function (g) { return g[1].toUpperCase(); })}%>

<template name="REMAX_TPL_<%=templateId%>_<%=map[id] || id%>">
  <<%=id%> <% for(let key of props) { %> <%=key%>="{{_h.v(i.p.<%=map[key] || camelCased(key)%>)}}" <% } %>>
    <% if (id === 'swiper') { %>
    <!-- c: children, i: id  -->
    <block wx:for="{{i.c}}" wx:key="{{i}}">
      <swiper-item key="{{item.p.key}}">
        <block wx:for="{{item.c}}" wx:key="{{i}}">
          <template is="REMAX_TPL_<%= templateId + 1%>_CONTAINER" data="{{i: item}}" />
        </block>
      </swiper-item>
    </block>
    <% } else { %>
    <block wx:for="{{i.c}}" wx:key="{{i}}">
      <template is="REMAX_TPL_<%= templateId + 1%>_CONTAINER" data="{{i: item}}" />
    </block>
    <% } %>
  </<%=id%>>
</template>