{
  "version": 2,
  "name": "remax-website",
  "builds": [
    {
      "src": "website/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/umi.js",
      "dest": "/website/umi.js"
    },
    {
      "src": "/umi.css",
      "dest": "/website/umi.css"
    }
  ]
}
